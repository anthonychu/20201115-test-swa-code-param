{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
      "callback": {
          "matchCondition": {
              "methods": [ "GET" ],
              "route": "/api/callback"
          },
          "backendUri": "http://localhost/api/HttpTrigger?_code={request.querystring.code}",
          "requestOverrides": {
            "backend.request.querystring.code": ""
          }
      }
  }
}